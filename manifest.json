{
  "name": "Page Panner",
  "version": "1.0",
  "description": "PDF-like panning on any webpage.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "grabbing.svg"
  }
}
